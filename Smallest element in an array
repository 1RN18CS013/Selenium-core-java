import java.util.Scanner;
public class smallestarrayelement {
    public static void main(String[] args) {
        int i,n,small,j;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the number of elements in a array");
        n=sc.nextInt();
        int a[]=new int[n];
        System.out.println("Enter the array elements");
        for(i=0;i<n;i++)
            a[i]=sc.nextInt();
        small=a[0];
        for(i=1;i<n;i++){
            
           
                if(a[i]<small)
                    small=a[i];
            
        }
        System.out.println("smallest element in an array is:"+ small);
    }
    
}
