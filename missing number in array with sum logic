import java.util.Scanner;
public class arraymissing {
    public static void main(String[] args) {
     int i,n,num,sumact=0,sumex;
     Scanner sc=new Scanner(System.in);
     System.out.println("Enter the no of elements in an array");
     n=sc.nextInt();
     int a[]=new int[n-1];
     System.out.println("Enter the elements with missing number");
     for(i=0;i<n-1;i++)
        a[i]=sc.nextInt();
     sumex=n*(n+1)/2;
     for(i=0;i<n-1;i++)
     {
         sumact+=a[i];
     }
     int mnumber=sumex-sumact;
     System.out.println("missing number in an array is:"+mnumber);
       
    }
     
}
